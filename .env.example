# API key used to validate the X-API-Key header for protected endpoints.
JANUS_API_KEY=dev-secret-change-me

# API key used to authenticate requests to the OpenAI API (when janus.llm.provider=openai).
OPENAI_API_KEY=sk-proj-REPLACE_WITH_YOUR_OPENAI_KEY

# API key used to authenticate requests to the Gemini API (when janus.llm.provider=gemini).
GEMINI_API_KEY=AIzaSyREPLACE_WITH_YOUR_GEMINI_KEY

# Connectors (You can define all connectors JANUS can access here or hardcode them in application.yaml)

JANUS_CONNECTORS_X_SOURCES_PROJECT_JDBC_URL=CHANGEME
JANUS_CONNECTORS_X_SOURCES_PROJECT_USERNAME=CHANGEME
JANUS_CONNECTORS_X_SOURCES_PROJECT_PASSWORD=CHANGEME
